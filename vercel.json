{
  "version": 2,
  "cleanUrls": false,
  "trailingSlash": false,
  "builds": [
    {
      "src": "vercel_app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }
  ],
  "routes": [
    {
      "src": "/index.html",
      "dest": "vercel_app.py"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ]
}
